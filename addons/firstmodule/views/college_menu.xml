<?xml version="1.0" encoding="UTF-8" ?>
<!-- This is our menu view -->
<odoo>
    <!--main menu-->
    <menuitem id="menu_module_1_root" name="Module 1" sequence="1" />
    <!--sub menu without action: will show a list of action menus-->
    <menuitem id="menu_module_1_student" name="Student" parent="menu_module_1_root" sequence="1" />
    <!--sub menu with action: will redirect different elements when clicked-->
    <menuitem id="menu_module_1_student_student" name="Student" parent="menu_module_1_student" action = "action_college_erp" sequence="1" />
    <menuitem id="menu_module_1_student_admission" name="Admission" parent="menu_module_1_student" action = "action_college_erp" sequence="2" />
</odoo>